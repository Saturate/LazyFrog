{
	"name": "lazyfrog",
	"version": "0.16.0",
	"description": "Sword & Supper game automation extension with TypeScript and React",
	"scripts": {
		"build": "wxt build",
		"dev": "wxt dev",
		"build:firefox": "wxt build -b firefox",
		"zip": "wxt zip",
		"zip:firefox": "wxt zip -b firefox",
		"clean": "rm -rf .wxt dist .output",
		"type-check": "tsc --noEmit",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"package": "pnpm run build && pnpm run zip && pnpm run copy-to-website",
		"copy-to-website": "mkdir -p ../website/public/downloads && cp dist/*.zip ../website/public/downloads/",
		"start:chrome": "wxt dev -b chrome --open https://www.reddit.com/r/SwordAndSupperGame/"
	},
	"keywords": [
		"browser-extension",
		"reddit",
		"game-automation",
		"typescript",
		"react"
	],
	"author": "Allan Kimmer Jensen",
	"license": "MIT",
	"dependencies": {
		"@devvit/protos": "^0.12.1",
		"@lazyfrog/types": "workspace:*",
		"@lazyfrog/ui": "workspace:*",
		"@tanstack/react-table": "^8.21.3",
		"@tanstack/react-virtual": "^3.13.12",
		"express": "^5.1.0",
		"lucide-react": "^0.546.0",
		"protobufjs": "^7.5.4",
		"query-selector-shadow-dom": "^1.0.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"xstate": "^5.23.0"
	},
	"devDependencies": {
		"@types/chrome": "^0.0.256",
		"@types/node": "^20.10.5",
		"@types/query-selector-shadow-dom": "^1.0.4",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@types/webextension-polyfill": "^0.10.7",
		"@vitest/ui": "^3.2.4",
		"@wxt-dev/module-react": "^1.1.5",
		"better-sqlite3": "^12.4.1",
		"jsdom": "^27.0.1",
		"prettier": "3.6.2",
		"sqlite3": "^5.1.7",
		"typescript": "^5.3.3",
		"vitest": "^3.2.4",
		"web-ext": "^9.0.0",
		"wxt": "^0.20.11"
	},
	"packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
